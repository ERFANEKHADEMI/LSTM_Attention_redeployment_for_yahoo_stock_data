{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "type": "string"
    },
    "subnetName": {
      "type": "string"
    },
    "amlWorkspaceName": {
      "type": "string"
    },
    "privateEndpointName": {
      "type": "string"
    }
  },
  "variables": {
    "location": "eastus2"
  },
  "resources": [
    {
      "type": "Microsoft.Network/privateEndpoints",
      "apiVersion": "2020-06-01",
      "name": "[parameters('privateEndpointName')]",
      "location": "[variables('location')]",
      "properties": {
        "subnet": {
          "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('vnetName'), parameters('subnetName'))]"
        },
        "privateLinkServiceConnections": [
          {
            "name": "myAmlPrivateLinkServiceConnection",
            "properties": {
              "privateLinkServiceId": "[resourceId('Microsoft.MachineLearningServices/workspaces', parameters('amlWorkspaceName'))]",
              "groupIds": ["amlworkspace"],
              "requestMessage": "Please approve my connection to AML Workspace"
            }
          }
        ],
        "manualPrivateLinkServiceConnections": []
      }
    }
  ]
}

